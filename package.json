{
  "name": "brophy.org",
  "description": "Matt Brophy's personal website (brophy.org)",
  "version": "1.2.1",
  "private": true,
  "author": "Matt Brophy <matt@brophy.org>",
  "license": "MIT",
  "homepage": "https://github.com/brophdawg11/brophy.org",
  "main": "build.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/brophdawg11/brophy.org.git"
  },
  "bugs": {
    "url": "https://github.com/brophdawg11/brophy.org/issues"
  },
  "keywords": [
    "brophy",
    "software",
    "web",
    "development",
    "javascript",
    "html",
    "css",
    "angular",
    "node"
  ],
  "scripts": {
    "build": "npm run build:client && npm run build:server && npm run build:middleware",
    "build:all": "CONFIG=all npm run _webpack --",
    "build:client": "CONFIG=client npm run _webpack --",
    "build:server": "CONFIG=server npm run _webpack --",
    "build:middleware": "CONFIG=middleware npm run _webpack --",
    "clean": "rm -rf dist && npm run fb:clean",
    "clean:all": "npm run clean && rm -rf node_modules",
    "dev": "NODE_ENV=local npm run build && NODE_ENV=local npm run start",
    "fb": "firebase",
    "fb:build": "npm run clean && npm run build && npm run fb:copy",
    "fb:build:prod": "npm run clean && NODE_ENV=production npm run build && npm run fb:copy",
    "fb:clean": "rm -rf functions/src && rm -rf functions/dist",
    "fb:copy": "npm run fb:copy:server && npm run fb:copy:create && npm run fb:copy:index && npm run fb:copy:dist && npm run fb:copy:static",
    "fb:copy:server": "mkdir -p functions/src/server && cp src/server/server.js functions/src/server/",
    "fb:copy:create": "mkdir -p functions/src/server && cp src/server/create-server.js functions/src/server/",
    "fb:copy:index": "mkdir -p functions/src && cp src/index.tpl.html functions/src/",
    "fb:copy:static": "mkdir -p public && cp -R static public/",
    "fb:copy:dist": "npm run fb:copy:dist:public && npm run fb:copy:dist:functions",
    "fb:copy:dist:public": "mkdir -p public && cp -R dist public/ && rm -f public/dist/*.json && rm -f public/dist/vue-middleware.js",
    "fb:copy:dist:functions": "mkdir -p functions/dist && cp -R dist/vue-middleware.js functions/dist/",
    "fb:deploy": "node build/verify-firebase-deps.js && firebase deploy",
    "fb:serve": "firebase serve -p 8080",
    "lint": "eslint --ext .js,.vue .",
    "nuke": "npm run clean && rm -rf node_modules && rm -rf .icon_cache && npm install",
    "test": "jest",
    "precommit": "npm run lint && npm run test",
    "start": "node src/server/dev-server.js",
    "_webpack": "webpack --config build/webpack.$CONFIG.config.js --progress --hide-modules"
  },
  "dependencies": {
    "compression": "1.7.3",
    "express": "4.16.4",
    "lodash-es": "4.17.11",
    "logatim": "0.9.0",
    "marked": "0.6.1",
    "vague-time": "2.4.2",
    "vue": "2.6.7",
    "vue-cookie": "1.1.4",
    "vue-router": "3.0.2",
    "vue-server-renderer": "2.6.7",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "base64-inline-loader": "1.1.1",
    "cheerio": "1.0.0-rc.2",
    "css-loader": "2.1.0",
    "cssnano": "4.1.10",
    "eslint": "5.14.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-vue": "5.2.2",
    "firebase-tools": "6.4.0",
    "highlight.js": "9.14.2",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "jest-serializer-vue": "2.0.2",
    "lodash": "4.17.11",
    "mkdirp": "0.5.1",
    "node-sass": "4.11.0",
    "postcss-loader": "3.0.0",
    "reading-time": "1.1.3",
    "sass-loader": "7.1.0",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "url-loader": "1.1.2",
    "vue-jest": "3.0.3",
    "vue-loader": "15.6.4",
    "vue-template-compiler": "2.6.7",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.3",
    "webpack-dev-middleware": "3.6.0",
    "webpack-hot-middleware": "2.24.3",
    "webpack-hot-server-middleware": "0.6.0",
    "webpack-merge": "4.2.1",
    "webpack-visualizer-plugin": "0.1.11",
    "yaml-front-matter": "4.0.0"
  }
}
