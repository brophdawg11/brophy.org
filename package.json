{
  "name": "brophy.org",
  "description": "Matt Brophy's personal website (brophy.org)",
  "version": "1.0.0",
  "private": true,
  "author": "Matt Brophy <matt@brophy.org>",
  "license": "MIT",
  "homepage": "https://github.com/brophdawg11/brophy.org",
  "main": "build.js",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "nuxt build",
    "generate": "NODE_ENV=production nuxt generate",
    "generate-dev": "nuxt generate",
    "dev": "nuxt",
    "deploy": "npm run clean && npm run build && npm run sync",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "nuke": "npm run clean && rm -rf node_modules && rm -rf .icon_cache && npm install",
    "precommit": "npm run lint",
    "serve": "cd dist && python -m SimpleHTTPServer",
    "start": "nuxt start",
    "sync": "aws --profile brophy.org s3 sync dist/. s3://brophy.org/ --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers --size-only",
    "watch": "npm-watch"
  },
  "watch": {
    "lint": {
      "patterns": [
        "components",
        "js",
        "layouts",
        "pages",
        "plugins",
        "store",
        "nuxt.config.js"
      ],
      "extensions": "js,vue,scss"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brophdawg11/brophy.org.git"
  },
  "bugs": {
    "url": "https://github.com/brophdawg11/brophy.org/issues"
  },
  "keywords": [
    "brophy",
    "software",
    "web",
    "development",
    "javascript",
    "html",
    "css",
    "angular",
    "node"
  ],
  "dependencies": {
    "@nuxtjs/axios": "4.5.2",
    "cheerio": "1.0.0-rc.2",
    "highlight.js": "9.12.0",
    "lodash": "4.17.4",
    "marked": "0.3.9",
    "moment": "2.20.1",
    "nuxt": "1.0.0-rc11",
    "nuxtent": "1.2.2",
    "reading-time": "1.1.0",
    "vue": "2.4.4",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "eslint": "4.18.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "4.0.2",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-vue": "4.3.0",
    "husky": "0.14.3",
    "lodash": "4.17.5",
    "marked": "0.3.16",
    "moment": "2.20.1",
    "node-sass": "4.7.2",
    "npm-watch": "0.3.0",
    "sass-loader": "6.0.6",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0"
  }
}
