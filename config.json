{
  "plugins": [
    "./plugins/paginator.coffee",
    "./node_modules/wintersmith-nunjucks",
    "wintersmith-libsass"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "nunjucks": {
    "filtermodules": [
      "nunjucks-markdown-filter"
    ]
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "node-sass": {
    "includePaths": [
        "./node_modules/foundation-sites/scss",
        "./node_modules/font-awesome/scss"
    ]
  },
  "paginator": {
    "perPage": 3
  },
