{
    "hosting": {
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "headers": [{
            "source": "/dist/*.js",
            "headers": [{
                "key": "Cache-Control",
                "value": "public, max-age=60, s-maxage=300"
            }]
        }, {
            "source": "/static/**/*.@(jpg|jpeg|gif|png)",
            "headers": [{
                "key": "Cache-Control",
                "value": "public, max-age=3600, s-maxage=86400"
            }]
        }],
        "public": "public",
        "port": 8080,
        "rewrites": [{
            "source": "**",
            "function": "server"
        }]
    }
}
